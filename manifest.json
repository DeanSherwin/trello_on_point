  {
    "name": "Trello PowerUp",
    "version": "0.0.1",
    "description": "Trello PowerUps for Free",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://trello.com/*"],
          "js": ["main.js"],
          "css": ["main.css"]
        }
      ],
    "permissions": [
      "activeTab"
    ],
    "icons": {
        "128": "./assets/top-icon.png"
    }
  }
